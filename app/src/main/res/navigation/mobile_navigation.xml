<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_menu">

    <fragment
        android:id="@+id/navigation_splash"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Splash"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_navigation_splash_to_navigation_login"
            app:destination="@id/navigation_login" />
    </fragment>
    <fragment
        android:id="@+id/navigation_login"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_navigation_login_to_navigation_registrar"
            app:destination="@id/navigation_registrar" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_inicio"
            app:destination="@id/navigation_inicio" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_olvidastes"
            app:destination="@id/navigation_olvidastes" />
    </fragment>
    <fragment
        android:id="@+id/navigation_registrar"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Registrar"
        android:label="fragment_registrar"
        tools:layout="@layout/fragment_registrar" >
        <action
            android:id="@+id/action_navigation_registrar_to_navigation_verificar"
            app:destination="@id/navigation_validar" />
    </fragment>
    <fragment
        android:id="@+id/navigation_validar"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Validar"
        android:label="fragment_validar"
        tools:layout="@layout/fragment_validar">
        <action
            android:id="@+id/action_navigation_verificar_to_navigation_passwordd"
            app:destination="@id/navigation_passwordd" />
    </fragment>
    <fragment
        android:id="@+id/navigation_inicio"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Inicio"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" >
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_favoritos"
            app:destination="@id/navigation_favoritos" />
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_menu"
            app:destination="@id/navigation_menu" />
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_perfil"
            app:destination="@id/navigation_perfil" />
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_carrito"
            app:destination="@id/navigation_carrito" />
    </fragment>
    <fragment
        android:id="@+id/navigation_menu"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Menu"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_navigation_menu_to_navigation_menuProducto"
            app:destination="@id/navigation_menuProducto" />
    </fragment>
    <fragment
        android:id="@+id/navigation_favoritos"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Favoritos"
        android:label="fragment_favoritos"
        tools:layout="@layout/fragment_favoritos" />
    <fragment
        android:id="@+id/navigation_perfil"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Perfil"
        android:label="fragment_perfil"
        tools:layout="@layout/fragment_perfil" />
    <fragment
        android:id="@+id/navigation_carrito"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Carrito"
        android:label="fragment_carrito"
        tools:layout="@layout/fragment_carrito" />
    <fragment
        android:id="@+id/navigation_menuProducto"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.MenuProducto"
        android:label="fragment_menu_producto"
        tools:layout="@layout/fragment_menu_producto" >
        <action
            android:id="@+id/action_navigation_menuProducto_to_navigation_menuProductoPersonalizar2"
            app:destination="@id/navigation_menuProductoPersonalizar" />
    </fragment>
    <fragment
        android:id="@+id/navigation_menuProductoPersonalizar"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.MenuProductoPersonalizar"
        android:label="fragment_menu_producto_personalizar"
        tools:layout="@layout/fragment_menu_producto_personalizar" />
    <fragment
        android:id="@+id/navigation_olvidastes"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Olvidastes"
        android:label="fragment_olvidastes"
        tools:layout="@layout/fragment_olvidastes" >
        <action
            android:id="@+id/action_navigation_olvidastes_to_navigation_verificar"
            app:destination="@id/navigation_validar" />
    </fragment>
    <fragment
        android:id="@+id/navigation_passwordd"
        android:name="pe.edu.senati.apkcavoshcafes101.ui.Passwordd"
        android:label="fragment_passwordd"
        tools:layout="@layout/fragment_passwordd" />
</navigation>